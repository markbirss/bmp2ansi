{
  "name": "bmp2ansi",
  "version": "1.0.0",
  "description": "convert a BMP file into an xterm-256 ANSI block image",
  "author": "Robey Pointer <robeypointer@gmail.com>",
  "license": "Apache-2.0",
  "main": "lib/",
  "engines": {
    "node": ">= 4.2.0"
  },
  "dependencies": {
    "antsy": "^1.1.1",
    "source-map-support": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015-node4": "^1.0.0",
    "eslint": "^1.4.28"
  },
  "files": [
    "lib",
    "bin"
  ],
  "scripts": {
    "build": "eslint src test/src && babel --source-maps --out-dir lib src",
    "clean": "rm -rf lib",
    "distclean": "npm run clean && rm -rf node_modules",
    "prepublish": "npm run build"
  }
}
